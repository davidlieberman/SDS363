results <- matrix(nrow = 45, ncol = 20)
for (j in 1:20) {
for (i in 1:45) {
test_point <- data[i,]
train_data <- data[-i,]
knn_prediction <- as.vector(knn(train = train_data[,c(6,9)], test = test_point[,c(6,9)], cl = train_data[,10], k = j))
truth <- as.vector(data[i,10])
results[i,j] <- truth == knn_prediction
}
}
